{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!api|assets|sw.js|_next|favicon.ico|static).*)",
      "destination": "/index.html"
    }
  ],
  "images": {
    "sizes": [
      400,
      640,
      828,
      1080,
      1200
    ],
    "domains": [],
    "minimumCacheTTL": 60
  }
}
