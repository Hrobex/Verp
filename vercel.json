{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!api|assets|sw.js|_next|favicon.ico|static).*)",
      "destination": "/index.html"
    }
  ],
  "images": {
    "sizes": [
      384,
      640,
      768,
      1024,
      1280
    ],
    "domains": [],
    "minimumCacheTTL": 60
  }
}
